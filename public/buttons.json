{
  "name": "aetheria-gothic-purple",
  "version": "1.0.0",
  "description": "9-slice button frames (PNG RGBA) for Aetheria UI. Designed for border-image / 9-slice scaling.",
  "recommendedAssetSizePx": {
    "width": 512,
    "height": 192
  },
  "slicePx": {
    "left": 96,
    "right": 96,
    "top": 64,
    "bottom": 64,
    "note": "Corners/ornament live in the sliced regions. Center is safe for text. Adjust if you re-render at different resolution."
  },
  "states": {
    "normal": {
      "png": "assets/ui/buttons/btn_gothic_purple_normal.png",
      "capInsetsPx": {
        "left": 96,
        "right": 96,
        "top": 64,
        "bottom": 64
      },
      "css": {
        "borderWidthPx": 32,
        "borderImageSlice": "64 96 64 96 fill",
        "borderImageWidth": "32px",
        "borderImageRepeat": "stretch"
      }
    },
